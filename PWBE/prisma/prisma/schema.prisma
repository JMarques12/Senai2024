// Looking for ways to speed up your queries, or scale easily with your serverless or edge functions?
// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Destino {
  id    Int    @id @default(autoincrement())
  nome  String
  valor Float
  data  DateTime
}

model PontoTuristico {
  id       Int     @id @default(autoincrement())
  nome     String
  endereco String
  telefone String
  valor    Float
}

model Hotel {
  id         Int     @id @default(autoincrement())
  nome       String
  valor      Float
  avaliacao  Float
  email      String
  site       String
}
